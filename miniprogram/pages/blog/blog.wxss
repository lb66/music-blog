.header{
  display: flex;
  align-items: center;
  padding: 0 20rpx;
  background-color: #fff;
  height: 100rpx;
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  z-index: 99;
  box-sizing: border-box;
}
.icon-write{
  font-size: 48rpx;
}
.publish-wrapper{
  width: 75rpx;
  height: 75rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.search-container{
  flex-grow: 1;
}
.blog-list{
  margin-top: 120rpx;
}
.blog-card{
  margin-bottom: 20rpx;
  box-sizing: border-box;
}